package bt.core.collections.iterators;

public class FibIteratorCopy implements Iterator {
	private int n = 0;
	private int fnm1 = 0;
	private int fn = 0;
	private int curr = 0;
	private int lim = 0;
	
	public FibIteratorCopy(int lim) {
		this.lim = lim;
	}
	
	@Override
	public void previous() {

	}

	@Override
	public void next() {
		if( n <= 1) {
			fn = curr = 1;
			n++;
		}else {
			fnm1 = fn;
			fn = curr;
			curr = fnm1 + fn;
			n++;
		}
	}

	@Override
	public Object current() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public void first() {
		curr = 0;
		n = 0;
	}

	@Override
	public void last() {
		
	}

	@Override
	public boolean isDone() {
		return fnm1 + fn > lim;
	}

}
