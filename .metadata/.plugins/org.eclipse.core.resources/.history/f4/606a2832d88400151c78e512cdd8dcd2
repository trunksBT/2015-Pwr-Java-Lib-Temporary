    }

    public boolean Remove(int aElement)
    {
        int _SetCopy = _val;
        int retVal = _SetCopy %= BASE;
        _val -= retVal;
        _val /= BASE;
        
        return retVal;
    }

    public void Add (int aElement)
    {
        _val*=BASE;
        _val+=aElement;
    }
    
    public boolean equals(SetNatural aVal)
    {
        return _val == aVal._val;
    }
    
    public String toString()
    {
        return "S:"+_val;
    }  
}