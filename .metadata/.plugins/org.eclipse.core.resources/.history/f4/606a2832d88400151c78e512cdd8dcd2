package Bt.Core.Collections;

public class SetNatural
{
    final int BASE = 10;
    int _val;
    QueueNatural _queue;

    public SetNatural(int aQueue)
    {
    	_queue = new QueueNatural(aQueue);
    	//TODO filterQueue With Set Methods
    }
    
