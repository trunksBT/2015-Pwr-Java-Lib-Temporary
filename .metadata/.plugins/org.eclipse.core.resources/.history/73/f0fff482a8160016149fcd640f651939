package bt.core.collections.lists;

import bt.core.collections.interfaces.List;
import bt.core.collections.iterators.Iterator;

public abstract class DirtyListDecorator implements List{
	private List tape;
	
	public DirtyListDecorator(List tape) {
		if( tape != null )
			this.tape = tape;
		else
			this.tape = new LinkedList();
	}
	
	public abstract isDirty();
}
