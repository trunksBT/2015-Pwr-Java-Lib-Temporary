package bt.core.algorithms;

public class Sequences {
	public static boolean isNotAscending( int[] chain ) {
		int i = -1;
		boolean methodPred = true;
		int size = chain.length;

		while(i++ < size-2) {
			if(chain[i] >= chain[i+1])
				methodPred &= true;
			else
				methodPred &= false;
		}		
		return methodPred;
	}
		
