package bt.collections.trees;

public class RatNode2 {
	int val;
	RatNode2 left,right;
	public int ammOfInternalNodes;
	
	public RatNode2(int val) {
		this(val,null,null);
	}

	public RatNode2(int val, RatNode2 left, RatNode2 right) {
		this.val = val;
		this.left = left;
		this.right = right;
	}
}
