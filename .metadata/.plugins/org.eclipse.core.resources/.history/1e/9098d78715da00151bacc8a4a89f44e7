package bt.core.collections.iterators;

public class FibonacciIterator implements Iterator{
	final int upperLim;
	final int fstVal;
	final int sndVal;
	
	int lastVal;
	int curr;
	
	public FibonacciIterator(int upperLim) {
		this.upperLim = upperLim;
		this.fstVal = 1;
		this.sndVal = fstVal;
		this.curr = -1;
	}
	
	@Override
	public void previous() {
		// TODO Auto-generated method stub	
	}

	@Override
	public void next() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public Object current() {
		// TODO Auto-generated method stub
		return curr+ lastVal;
	}

	@Override
	public void first() {
		curr = fstVal;
	}

	@Override
	public void last() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public boolean isDone() {
		// TODO Auto-generated method stub
		return false;
	}

}
