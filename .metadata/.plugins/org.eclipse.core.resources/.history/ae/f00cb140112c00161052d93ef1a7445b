package bt.collections.trees;

public class RatNode2 {
	RatNode left,right;
	int val;
	int ammOfLowerNodes;
	int ammOfInnerNodes;
	
	public RatNode2(int val) {
		this(val,null,null);
	}
	
	public RatNode2(int val, RatNode left, RatNode right) {
		this.left = left;
		this.right = right;
		this.val = val;
	}
}
