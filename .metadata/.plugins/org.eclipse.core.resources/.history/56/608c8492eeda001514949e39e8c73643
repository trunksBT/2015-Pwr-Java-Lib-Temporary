package bt.core.algorithms;

public class Arrays {
	public static int sumValsOnMultiOfK( int [][] mac, int mult ) {
		int sum = 0;
		int size = mac.length;
		
		for(int i = 0 ; i<size; i++)
			for(int j = ( mult - (i % mult)) % mult; j<size; j+=mult )
			{
				int x1 = i % mult;
				int x2 = mult - (i % mult);
				int x3 = ( mult - (i % mult)) % mult;
				
				sum+= mac[i][j];
			}
		
		return sum;
	}
}
