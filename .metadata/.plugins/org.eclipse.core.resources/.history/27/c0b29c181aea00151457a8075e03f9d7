package bt.tests.core.collections.dataTypes;

import bt.core.collections.dataTypes.GuardedLinkedListDouble;
import bt.core.collections.interfaces.List;
import bt.core.exceptions.EmptyQueueException;

public class Queue implements bt.core.collections.interfaces.Queue {
	private final List list;

	public Queue(List list) {
		this.list = list;
	}
	
	public Queue() {
		this.list = new GuardedLinkedListDouble();
	}

	@Override
	public void enqueue(Object val) {
		// TODO Auto-generated method stub
	}

	@Override
	public Object dequeue() throws EmptyQueueException {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public void clear() {
		list.clear();
	}

	@Override
	public int size() {
		return list.size();
	}

	@Override
	public boolean isEmpty() {
		return list.isEmpty();
	}
}
