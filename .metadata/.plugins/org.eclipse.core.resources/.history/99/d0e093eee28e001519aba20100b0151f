package Bt.Core.Collections;
import java.util.*;

public class Tab {
	
	int []tab;
	int n;
	
	Tab(int aSize)
	{
		n = aSize;
		tab = new int[n];
	}
	
	Tab(int aSize,int []in)
	{
		n = aSize;
		tab = in;
	}
		
	void wczytaj()
	{
		Scanner scanner = new Scanner(System.in);
		n = 0;
		
		while(scanner.hasNextInt())
		{
			tab[n++]=scanner.nextInt();
		}
	}
	
	void usunPowt()
	{
		for(int i = 0 ;i<n;i++)
		{
			int temp = tab[i];
			
			int []tempTab;
			int wrongPlace = -1;
			
			for (int j = i+1; j < n; j++) {	
				if(temp == tab[j])
				{
					wrongPlace = j;
				}
				
				if(wrongPlace != -1)
				{
					tempTab = new int[n-1];
					
					for(int k = 0 ;k<wrongPlace;k++)
					{
						tempTab[k]=tab[k];
						// skonczylem na przedostatnim
					}
					
					int startPlace = wrongPlace;
					
					for(int l = startPlace;l<n;l++)
					{
						tempTab[l] = tab[++startPlace];
					}
				}
			}
			
			
		}
	}
	
	void druk()
	{
	}
}
