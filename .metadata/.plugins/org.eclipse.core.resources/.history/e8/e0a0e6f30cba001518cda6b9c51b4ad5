package bt.core.collections.elems;

import java.io.Serializable;

import bt.core.collections.interfaces.IPrintable;
import bt.core.streams.Strumien;

public class Pracownik implements IPrintable,Strumien
{
    double x;
    double y;

    public Pracownik(Pracownik p)
    {
        this.x=p.x;
        this.y=p.y;
    }
    
    public Pracownik(double x,double y)
    {
    	this.x=x;
    	this.y=y;
    }
      
    public boolean equals(Pracownik second)
    {
        return x == second.x && y == second.y;
    }
        
    @Override
	public void drukuj() {
		wyj.println(String.format("( %#5.1f,%#5.1f )",x,y));
	}
 }  

