package bt.core.algorithms;

public class Sequences {
	public static boolean isNotAscending( int[] chain ) {
		int i = 0;
		int n = chain.length;
		boolean flag = true;
		
		while(true) {
			boolean logic = i < n-1 && chain[i] <= chain[n+1];
			if (logic)
				i++;
			else
				flag = false;
		}
		
		return i >= n-1;
	}
}
