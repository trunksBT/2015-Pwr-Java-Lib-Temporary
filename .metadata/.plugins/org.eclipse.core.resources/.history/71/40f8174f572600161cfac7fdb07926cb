package bt.collections.lists;

public class RatajElem2 {
	int val;
	RatajElem2 next;
	
	public RatajElem2(int val, RatajElem2 next) {
		this.val = val;
		this.next = next;
	}

	public RatajElem2(int val) {
		this(val,null);
	}

	public void setNext(RatajElem2 newNext) {
		this.next = newNext;
	}
	
	public int getVal() {
		return val;
	}

}
