package bt.core.collections.dataTypes.graphs;

import java.util.List;

public interface Graph<V,S> {
	public int numberOfVerts();
	public int numberOfEdges();
	public void addEdge(V vertFrom, V vertTo);
	public Iterable<V> adjacentVerts(V vert);
	public List<V> getVertices();
	public S Krawedz(V vertFrom, V vertTo);
	public List<V> Krawedzie(V w);
}
