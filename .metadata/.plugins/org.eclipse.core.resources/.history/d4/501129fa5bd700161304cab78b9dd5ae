		wierzcholki_ = wierzcholki;
	}
	
	@Override
	public List<W> wierzcholki() {
		return wierzcholki_;
	}

	@Override
	public List<IKrawedz<W, S>> krawedzie() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public List<IKrawedz<W, S>> krawedzie(W w) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public S czyKrawedz(W w1, W w2) {
		// TODO Auto-generated method stub
		return null;
	}

}
