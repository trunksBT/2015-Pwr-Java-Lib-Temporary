package Bt.Core.Geometry.Elements.ToPrint;

import Bt.Core.Geometry.Elementaries.Point;

public class PointPrinter {

	Point _buff;
	
	public PointPrinter(Point aPoint) {
		_buff = aPoint;
	}
	
	@Override
    public String toString()
    {
		String ret = String.format("%3f",_buff.GetX());
//		String ret = String.format("%-4s_%-2s","val","val2");
		
        return ret;
    }
}
