package Bt.Core.Collections;
import java.util.*;

public class OneDimTable {
	
	int []tab;
	int size;
	
	OneDimTable(int size,int []tab)
	{
		this.size = size;
		this.tab = new int[this.size];
		this.tab = tab;
	}

		
	void wczytaj()
	{
		Scanner scanner = new Scanner(System.in);
		int n = 0;
		
		while(scanner.hasNextInt())
		{
			tab[n++]=scanner.nextInt();
		}
	}
//	
