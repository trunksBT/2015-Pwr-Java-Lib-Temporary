//		{	
//			for (int j = i+1;j < n; ++j) {
//								
//				if(tab[i] == tab[j])
//				{
//					for(int l = j;l<n-1;)
//						tab[l] = tab[++l];
//					
//					--n;
//				}	
//			}
//		}
//	}
	
	void druk()
	{
		System.out.println("Tablica");
		
		for(int i = 0;i<size;i++)
		{
			for (int j = 0; j < size; j++) {
				System.out.print(tab[i]);
				System.out.println();
			}
		}
		
		System.out.println();
		System.out.println("Koniec");
	}
}
