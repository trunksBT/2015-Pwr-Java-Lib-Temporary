	public static boolean equals(RatajListRec2 expList, RatajListRec2 rcList, int inSize) {
		boolean retVal = true;
		
		for(int i = 0; i< inSize; i++)
			retVal = expList.getElem(i).val == rcList.getElem(i).val;
		
		return retVal;
	}

