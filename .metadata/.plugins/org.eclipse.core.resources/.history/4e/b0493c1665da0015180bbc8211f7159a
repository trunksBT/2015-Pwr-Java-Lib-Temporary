package bt.core.algorithms;

public class Sequences {
	public static boolean isNotAscending( int[] chain ) {
		int i = -1;
		boolean methodPred = false;
		int size = chain.length;
		
		if(size >= 2) {
			methodPred = true;
			while(i++ < size-1 && chain[i] <= chain[i+1])
				methodPred &= true;
		}
		
		return methodPred;
//		
//		
//		while(i < size-1 && chain[i] >= chain[i+1])
//				i++;
//		
//		return i <= size-1;
	}
}
